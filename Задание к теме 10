1 Задание
#include <iostream>
using namespace std;
int main() {
	int a,b;
	cin >> a >> b;
	if (a > 2 & b <= 3) {
		cout << "True";
	}
	else {
		cout << "False";
	}
}
2 Задание
#include <iostream>
using namespace std;
int main() {
	int a,b,c;
	cin >> a >> b >> c;
	if (a<b & b<c) {
		cout << "True";
	}
	else {
		cout << "False";
	}
}
3 Задание
#include <iostream>
using namespace std;
int main() {
	int a;
	cin >> a;
	if (a/10<10 & a/10>0 & a%2==0) {
		cout << "True";
	}
	else {
		cout << "False";
	}
}
4 Задание
#include <iostream>
using namespace std;
int main() {
	int  a,q,w,e;
	cin >> a;
	q = a / 100;
	w = (a % 100) / 10;
	e = a % 10;
	if (q>w & w>e || q < w & w < e) {
		cout << "True";
	}
	else {
		cout << "False";
	}
}
5 Задания
#include <iostream>
using namespace std;
int main() {
	int  a,q,w,e,r,s,d;
	cin >> a;
	q = a / 1000;
	w = (a % 1000) / 100;
	e = (a%100) / 10;
	r = a % 10;
	if (q*10+w == r*10+e) {
		cout << "True";
	}
	else {
		cout << "False";
	}
}
6 Задания
#include <iostream>
using namespace std;
int main() {
	int  a,b,c,a2,b2,c2;
	cin >> a >> b >> c;
	a2 = a * a;
	b2 = b * b;
	c2 = c * c;
	if (a2==b2+c2 || b2==a2+c2 || c2==a2+b2) {
		cout << "True";
	}
	else {
		cout << "False";
	}
}
7 Задания
#include <iostream>
using namespace std;
int main() {
	int  a,b,c,a2,b2,c2;
	cin >> a >> b >> c;
	if (a < (b + c) & b < (a + c) & c < (a + b)){
		cout << "True";
	}else {
		cout << "False";
	}
}
